    "bigi": "^1.4.0",
    "buffer-xor": "^1.0.3",
    "devp2p-dpt": "1.0.1",
    "ecurve": "^1.0.1",
    "ethereumjs-util": "2.4.0",
    "rlp": "2.0.0",
